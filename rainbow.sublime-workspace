tion(data) {\n      ko.mapping.fromJS(data, {}, this);\n\n    };\n\n    var schoolMappingOptions = {\n      create: function(options) {\n        return new SchoolVM(options.data);\n      },\n    };\n\n    // #endregion\n\n    var init = function() {\n\n      dataservice.sites.getSites()\n        .then(function(data) {\n\n          extractMetaData(data);\n\n          vm.sites(ko.mapping.fromJS(data, siteMappingOptions)());\n          vm.filteredSites(vm.sites());\n\n          ko.applyBindings(vm, $('#rainbow')[0]);\n        });\n\n      function extractMetaData(sites) {\n        var schoolAry = [];\n\n        // Extract out the list of schools and the count of each\n        for (var i = 0; i < sites.length; i++) {\n          var currSite = sites[i];\n          for (var j = 0; j < currSite.schools.length; j++) {\n            var currSchool = currSite.schools[j];\n            var foundSchool = _.findWhere(schoolAry, {\n              name: currSchool.name\n            });\n            if (foundSchool) {\n              // School is alreadyin the array. Up the count.\n              foundSchool.count += 1;\n            } else {\n              var newSchool = $.extend({}, currSchool, {\n                count: 1,\n                isChecked: false\n              });\n              schoolAry.push(newSchool);\n            }\n          }\n        }\n\n        vm.schools(ko.mapping.fromJS(schoolAry)());\n      }\n    };\n\n    var handleFilterChanged = function(data, event) {\n      filterSites();\n      return true;\n    };\n\n    var filterSites = function() {\n      // Filter the sites by school\n      var tmpSites = ko.observableArray([]); // Build up the filtered sites in a separate array to avoid extra DOM updates due to data binding\n\n      // First figure out which schools are checked\n      var schoolAry = ko.mapping.toJS(vm.schools());\n      schoolAry = _.chain(schoolAry)\n        .filter(function(item) {\n          return item.isChecked;\n        })\n        .pluck('name')\n        .value();\n\n      if (schoolAry.length === 0) {\n        // no items checked therefore return all sites\n        vm.filteredSites(vm.sites());\n        return;\n      }\n\n      for (var i = 0; i < vm.sites().length; i++) {\n        var currSchools = ko.mapping.toJS(vm.sites()[i].schools());\n        var schoolNames = _.pluck(currSchools, 'name');\n        var result = _.intersection(schoolAry, schoolNames);\n        if (result.length > 0) {\n          tmpSites.push(vm.sites()[i]);\n        }\n      }\n\n      vm.filteredSites(tmpSites());\n\n    };\n\n    var vm = {\n      sites: sites,\n      filteredSites: filteredSites,\n      schools: schools,\n      init: init,\n      handleFilterChanged: handleFilterChanged\n    };\n\n    return vm;\n  });",
